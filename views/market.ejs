<!DOCTYPE html>
<html>
    <%- include("./partials/head") %>
        <link href="/css/market-style.css" rel="stylesheet">
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
        <script>
            function myFunction() {
                var x = document.getElementById("myLinks");
                if (x.style.display === "block") {
                  x.style.display = "none";
                } else {
                  x.style.display = "block";
                }
              }
        </script>
    </head>
    <body>
        <%- include("./partials/header") %>
       
       
    <main id="main-market">
     
    <section class="posts">
     
      <% tokens.forEach(function(token){ %>
        <a href="/products/detail/<%= token.id %>">
          <img src="<%= token.image %>" class="nft" alt="nftMarket" class="tokenIMG"></a>
          <article>
            
            <p>Este token tiene lo siguiente:</p>
            <ul>
              <li>ID: #<%= token.id %> </li>
              <li>Nombre: <%= token.name %> </li>
              <li>Descripcion: <%= token.description %> </li>
              <li>Precio: Ξ<%= token.price %> </li>
            </ul>
          </article>
        </a>
        <% }) %> 
      </section>
     

     <div class="posts">
        
        <h5 id="hash">#3d #3dart</h5> <i class="far fa-heart"> </i>
        <img id="img-market" src="img/Onset.png">
        <h4>userName</h4>
        <h2>Title</h2>
        <p>0,00 Ξ</p>

     </div>
     <div class="posts">
        
        <h5 id="hash">#3d #3dart</h5> <i class="far fa-heart"> </i>
        <img id="img-market" src="img/Rightorder.png">
        <h4>userName</h4>
        <h2>Title</h2>
        <p>0,00 Ξ</p>

     </div>
     <div class="posts">
        
        <h5 id="hash">#3d #3dart</h5> <i class="far fa-heart"> </i>
        <img id="img-market" src="img/SR_Face_To_Face_dromsjel.png">
        <h4>userName</h4>
        <h2>Title</h2>
        <p>0,00 Ξ</p>

     </div>
        
    </main>
       
    <%- include("./partials/footer") %>
    </div>
</body>
</html>